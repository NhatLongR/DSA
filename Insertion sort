#include <iostream>
using namespace std;

void insertion_sort(int a[],int n)
{
for (i=1,i<n,i++
{
j=i-1;
t=a[i];
while ( t<a[j] && j>0)
{
a[j+1] =a[j];
j--;
}
a[j+1]=t;

}
}


int main()
{
int n,i;
int a[];
cin>>n;
for (i=0, i<n,i++)
{
cin>>a[i];
}

for (i=0,i<n,i++)
cout<<"a["<<i+1<<"]:" a[i];

insertion_sort(a[],n);
cout<<"Mang sau khi sap xep la :";
for (i=0,i<n,i++)
cout<<"a["<<i+1<<"]:" a[i];
}
